{
  "name": "csv-mapper-lite",
  "version": "1.0.3",
  "description": "A node.js library for parsing and mapping csv string into pre-defined format.",
  "scripts": {
    "lint": "./node_modules/eslint/bin/eslint.js -c .eslintrc.js --ignore-path .gitignore **/*.js",
    "test": "./node_modules/.bin/flow && ./node_modules/.bin/ava test ./src/test/*_spec.js",
    "test-debug": "node --inspect --debug-brk ./node_modules/ava/profile.js ./src/test/csv_spec.js",
    "test-watch": "npm test -- --watch"
  },
  "main": "index.js",
  "repository": "git@github.com:j1wu/csv-mapper-lite.git",
  "author": "j1wu <ji.wu@outlook.com>",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "eslint": "^4.0.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.49.1",
    "flow-copy-source": "^1.2.0",
    "ghooks": "^2.0.0",
    "rimraf": "^2.6.1"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test"
    }
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  }
}
